{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff] "
      }
    }
  },
  "Ai": {
    "DefaultTtlSeconds": 60
  },
  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://sbemulatorns:5672/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=SAS_KEY_VALUE;UseDevelopmentEmulator=true;"
  },
  "Jwt": {
    "Secret": "<PUT-YOUR-KEY-HERE>",
    "Issuer": "manager",
    "Audience": "client-app",
    "RefreshTokenHashKey": "<PUT-YOUR-KEY-HERE>"
  },
  "AllowedHosts": "*"
}