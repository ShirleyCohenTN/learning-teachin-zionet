{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "SingleLine": true,
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff] "
    }
},
  "Ai": {
    "DefaultTtlSeconds": 60
  },
  "AllowedHosts": "*"
}
