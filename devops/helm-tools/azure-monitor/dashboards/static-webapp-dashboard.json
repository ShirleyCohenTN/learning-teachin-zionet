{
  "title": "Static Web App - Application Insights Dashboard (Logs)",
  "uid": "static-webapp-insights",
  "timezone": "",
  "style": "dark",
  "tags": ["static-webapp","application-insights","azure"],
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "resource_group",
        "label": "Resource Group",
        "type": "query",
        "datasource": "Azure Monitor",
        "query": "ResourceGroups()",
        "definition": "ResourceGroups()",
        "refresh": 1,
        "hide": 0,
        "includeAll": false
      },
      {
        "name": "workspace",
        "label": "Log Analytics Workspace",
        "type": "query",
        "datasource": "Azure Monitor",
        "query": "ResourceNames($resource_group, Microsoft.OperationalInsights/workspaces)",
        "definition": "ResourceNames($resource_group, Microsoft.OperationalInsights/workspaces)",
        "refresh": 1,
        "hide": 0,
        "includeAll": false
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Page Views",
      "type": "timeseries",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "timeSeries",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where $__timeFilter(TimeGenerated)\n| summarize total=count() by bin(TimeGenerated, 5m)\n| order by TimeGenerated asc"
          }
        }
      ]
    },
    {
      "id": 2,
      "title": "Unique Users",
      "type": "timeseries",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "timeSeries",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where $__timeFilter(TimeGenerated)\n| summarize users=dcount(tostring(user_Id)) by bin(TimeGenerated, 5m)\n| order by TimeGenerated asc"
          }
        }
      ]
    },
    {
      "id": 3,
      "title": "Page Load Performance (avg ms)",
      "type": "timeseries",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "timeSeries",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where $__timeFilter(TimeGenerated)\n| summarize avg_ms=avg(DurationMs) by bin(TimeGenerated, 5m)\n| order by TimeGenerated asc"
          }
        }
      ]
    },
    {
      "id": 4,
      "title": "User Sessions",
      "type": "timeseries",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "timeSeries",
            "timeColumn": "TimeGenerated",
            "query": "AppSessions\n| where $__timeFilter(TimeGenerated)\n| summarize total=count() by bin(TimeGenerated, 5m)\n| order by TimeGenerated asc"
          }
        }
      ]
    },
    {
      "id": 5,
      "title": "Errors & Failed Requests",
      "type": "timeseries",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "timeSeries",
            "timeColumn": "TimeGenerated",
            "query": "AppExceptions\n| where $__timeFilter(TimeGenerated)\n| summarize exceptions=count() by bin(TimeGenerated, 5m)\n| order by TimeGenerated asc"
          }
        },
        {
          "refId": "B",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "timeSeries",
            "timeColumn": "TimeGenerated",
            "query": "AppRequests\n| where $__timeFilter(TimeGenerated)\n| where tostring(Success) == \"False\" or tobool(isnull(Success)) == false and Success == false\n| summarize failed=count() by bin(TimeGenerated, 5m)\n| order by TimeGenerated asc"
          }
        }
      ]
    },
    {
      "id": 6,
      "title": "Current Load Time",
      "type": "stat",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 0, "y": 24, "w": 6, "h": 4 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "table",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where $__timeFilter(TimeGenerated)\n| summarize avg_ms=avg(DurationMs)\n| project avg_ms"
          }
        }
      ]
    },
    {
      "id": 7,
      "title": "Active Users (1h)",
      "type": "stat",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 6, "y": 24, "w": 6, "h": 4 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "table",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where TimeGenerated >= ago(1h)\n| summarize users=dcount(tostring(user_Id))\n| project users"
          }
        }
      ]
    },
    {
      "id": 8,
      "title": "Total Page Views (1h)",
      "type": "stat",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 12, "y": 24, "w": 6, "h": 4 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "table",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where TimeGenerated >= ago(1h)\n| summarize total=count()\n| project total"
          }
        }
      ]
    },
    {
      "id": 9,
      "title": "Exceptions (1h)",
      "type": "stat",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 18, "y": 24, "w": 6, "h": 4 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "table",
            "timeColumn": "TimeGenerated",
            "query": "AppExceptions\n| where TimeGenerated >= ago(1h)\n| summarize total=count()\n| project total"
          }
        }
      ]
    },
    {
      "id": 10,
      "title": "Browser Usage",
      "type": "piechart",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 0, "y": 28, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "table",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where $__timeFilter(TimeGenerated)\n| summarize total=count() by Client_Browser\n| project Client_Browser, total"
          }
        }
      ]
    },
    {
      "id": 11,
      "title": "Users by Country",
      "type": "piechart",
      "datasource": "Azure Monitor",
      "gridPos": { "x": 12, "y": 28, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "queryType": "Logs",
          "azureLogAnalytics": {
            "workspace": "$workspace",
            "resultFormat": "table",
            "timeColumn": "TimeGenerated",
            "query": "AppPageViews\n| where $__timeFilter(TimeGenerated)\n| summarize total=count() by Client_CountryOrRegion\n| project Client_CountryOrRegion, total"
          }
        }
      ]
    }
  ],
  "annotations": { "list": [] },
  "schemaVersion": 38,
  "version": 1
}
